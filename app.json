{
    "expo": {
        "name": "arxiv-viewer",
        "slug": "arxiv-viewer",
        "privacy": "public",
        "platforms": [
            "ios",
            "android"
        ],
        "version": "1.1.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "description": "View new and recent papers from arXiv",
        "githubUrl": "https://github.com/olofos/arxiv-viewer.git"
    }
}